{
    "name": "wdalmut/frankie-twig-module",
    "description": "Add Twig to your project",
    "license": "MIT",
    "require": {
        "wdalmut/frankie-modular": "dev-master",
        "psr/container": "~1",
        "zendframework/zend-servicemanager": "~3",
        "twig/twig": "~2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Corley\\TwigModule\\": ["./src"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Corley\\TwigModule\\": ["./tests"]
        }
    }
}
